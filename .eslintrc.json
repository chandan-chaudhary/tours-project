
//to learn more about eslint visit official site http://eslint.org/docs
{
    "extends":["prettier", "plugin:node/recomended"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "erroe",
        "spaced-comment": "off",
        "no-console": "warn",
        "consistent-return":"off",
        "func-names":"off",
        "object-shorthand":"off",
        "no-process-exit":"off",
        "no-param-reassign":"off",
        "no-return-await":"off",
        "no-underscore-dangle":"off",
        "class-methods-use-this":"off",
        "prefer-destructuring":["error",{"object": true, "array": false}],
        "no-unused-vars": ["error",{"argsIgnorePattren": "req|res|next|val"}]
    }
}